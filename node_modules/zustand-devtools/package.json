    "typescript": "*",
    "zustand": "*"
  },
  "devDependencies": {
    "@types/node": "^20.6.2",
    "prettier": "^2.6.2",
    "typescript": "^4.9.3",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^1.7.1"
  },
  "keywords": [
    "zustand",
    "devtools",
    "action",
    "redux"
  ]
