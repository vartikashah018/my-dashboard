{
  "name": "zustand-devtools",
  "version": "1.1.0",
  "description": "names for zustand devtools",
  "type": "module",
  "author": "Alexeeev Alexander",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "license": "ISC",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/AlexeevSasha/zustand-devtools"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
